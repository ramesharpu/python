def bubble_sort(arr):
    n = len(arr)
    for i in range(n):
        for j in range(0, n-i-1):
            if arr[j]>arr[j+1]:
                arr[j], arr[j+1] = arr[j+1], arr[j]


arr = [4, 6, 1, 5, 8, 9, 2]
bubble_sort(arr)
# Print the sorted array
for i in range(len(arr)):
    print ("{}".format(arr[i]))
